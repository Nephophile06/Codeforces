#include <iostream>
using namespace std;

int main() 
{
    int n;
    cin >> n;

    int count = 0;
    for (int i = 0; i < n; i++)
    {
        if ( (n >= 1) && (n <= 100) ) 
        {
            int a;
            cin >> a;
            count = count + a;
        }
    }
    if (count == 0)
        cout << "EASY" << endl;
    else
        cout << "HARD" << endl;

    return 0;
}
