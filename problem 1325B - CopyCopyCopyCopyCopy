#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
 
    while (t--)
    {
        int n;
        cin >> n;
 
        unordered_set<long long> distinct;
        distinct.reserve(n * 2);
 
        for (int i = 0; i < n; i++)
        {
            long long x;
            cin >> x;
            distinct.insert(x);
        }
 
        cout << distinct.size() << endl;
    }
 
    return 0;
}
