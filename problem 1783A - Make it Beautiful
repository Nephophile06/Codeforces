#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
 
    while (t--)
    {
        int n;
        cin >> n;
 
        vector<int> a(n);
        for (int i = 0; i < n; i++)
            cin >> a[i];

        bool allEqual = true;
        for (int i = 1; i < n; i++)
        {
            if (a[i] != a[0])
            {
                allEqual = false;
                break;
            }
        }

        if (allEqual)
        {
            cout << "NO\n";
            continue;
        }

        cout << "YES" << endl;
        sort(a.begin(), a.end());

        cout << a[n - 1] << " ";
        for (int i = 0; i < n - 1; i++)
        {
            cout << a[i] << " ";
        }
        
        cout << endl;
    }

    return 0;
}
