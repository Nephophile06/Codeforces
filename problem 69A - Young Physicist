#include <iostream>
#define endl "\n"
using namespace std;

int main()
{
    int n;
    cin >> n;
    int a, b, c;
    int a1 = 0, b1 = 0, c1 = 0;
    for (int i = 0; i < n; i++)
    {
        cin >> a >> b >> c;
        a1 = a + a1;
        b1 = b + b1;
        c1 = c + c1;
    }
    if ((a1 == 0) && (b1 == 0) && (c1 == 0))
        cout << "YES" << endl;
    else 
        cout << "NO" << endl;
}
