#include <iostream>
#include <string>
using namespace std;

int main()
{
    string a, b, c;
    cin >> a >> b;

    int length = a.length();
    
    for (int i = 0; i < a.length(); i++)
    {
        if(a[i] == b[i]) 
            c = c + '0';
        else 
            c = c + '1';
    }

    cout << c << endl;

    return 0;
}
