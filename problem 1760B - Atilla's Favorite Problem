#include <iostream>
using namespace std;

int main() 
{
    int t;
    cin >> t;  

    while (t--) 
    {
        int n;
        cin >> n;    
        char max_char = 'a';

        for (int i = 0; i < n; i++) 
        {
            char c;
            cin >> c;
            if (c > max_char) 
                max_char = c;
        }

        cout << (max_char - 'a' + 1) << '\n';
    }

    return 0;
}
