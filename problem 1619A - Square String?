#include <iostream>
using namespace std;

int main()
{
    int t;
    cin >> t;
    string a;

    for (int i = 0; i < t; i++)
    {
        cin >> a;
        
        if (a.length() % 2 == 0)
        {
            string b = a.substr(0, a.length()/2);
            string c = a.substr(a.length()/2, a.length()/2);

            if (b == c)
                cout<< "YES"<<endl;
            else 
                cout<< "NO"<<endl;
        }
        
        else 
            cout<< "NO" << endl;
    }

    return 0;
}
