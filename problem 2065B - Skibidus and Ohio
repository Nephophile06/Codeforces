#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    
    while (t--)
    {
        string s;
        cin >> s;
    
        bool dup = false;
        for (int i = 0; i + 1 < s.length(); i++)
        {
            if (s[i] == s[i + 1])
            {
                dup = true;
                break;
            }
        }

        if (dup)
            cout << 1 << endl;
        else
            cout << s.length() << endl;
    }
    
    return 0;
}
