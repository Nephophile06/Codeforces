#include <iostream>
using namespace std;

int main()
{
    int T, a, b, c;
    cin >> T;

    for (int i = 0; i < T; i++)
    {
        cin >> a >> b >> c;
        if ( (a+b == c) || (b+c == a) || (c+a == b) )
            cout << "YES" << endl;
        else 
            cout << "NO" << endl;
    }

    return 0;
}
