#include <iostream>
using namespace std;
int main() 
{
    int t;
    cin >> t;
    int a, b;
    int mishka = 0, chris = 0;

    for (int i = 0; i < t; i++)
    {
        cin >> a >> b;

        if (a > b)
            mishka++;
        else if (b > a)
            chris++;
        else
            continue;
    }

    if (mishka > chris)
        cout << "Mishka" << endl;
    else if (mishka < chris)
        cout << "Chris" << endl;
    else 
        cout << "Friendship is magic!^^" << endl;

    return 0;
}
