#include <iostream>
using namespace std;
 
int main()
{
    int t;
    cin >> t;
 
    for (int i = 0; i < t; i++)
    {
        string n;
        cin >> n;
        int j;
 
        for (j = (n.length()-1); j >= 0; j--)
        {
            if (n[j] == 'p')
                n[j] = 'q';
            else if (n[j] == 'q')
                n[j] = 'p';
        
            cout << n[j];
        }
        cout << endl;
    }
}
