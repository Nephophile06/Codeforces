#include <iostream>
using namespace std;

int main()
{
    long long int n, k;
    cin >> n >> k;
    int a;
    for (int i = 0; i < k; i++)
    {
        a = n % 10;
        if (a == 0)
        {
            n = n / 10;
        }
        else 
        {
            n = n - 1;
        }
    }
    cout << n << endl;  
}
