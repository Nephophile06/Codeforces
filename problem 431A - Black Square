#include <iostream>
using namespace std;
int main() 
{
    int a1, a2, a3, a4, total = 0;
    cin >> a1 >> a2 >> a3 >> a4;
    string s;
    cin >> s;

    for (int i = 0; i < s.length(); i++)
    {
        if (s[i] == '1')
            total = total + a1;
        else if (s[i] == '2')
            total = total + a2;
        else if (s[i] == '3')
            total = total + a3;
        else if (s[i] = '4')
            total = total + a4;
    }

    cout << total << endl;
    return 0;
}
