#include <iostream>
using namespace std;
int main() 
{
    int T;
    cin >> T;
    int a, b, c;

    for (int i = 0; i < T; i++)
    {
        cin >> a >> b >> c;
        if (a+b == c)
            cout << "+" << endl;
        else if (a-b == c)
            cout << "-" << endl;
    }

    return 0;
}
